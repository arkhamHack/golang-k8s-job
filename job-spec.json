{
  "apiVersion": "batch/v1",
  "kind": "Job",
  "metadata": {
    "name": "example-job"
  },
  "spec": {
    "template": {
      "spec": {
        "containers": [
          {
            "name": "hello",
            "image": "busybox",
            "command": ["echo", "Hello from priority queue!"]
          }
        ],
        "restartPolicy": "Never"
      }
    },
    "backoffLimit": 0
  }
}
